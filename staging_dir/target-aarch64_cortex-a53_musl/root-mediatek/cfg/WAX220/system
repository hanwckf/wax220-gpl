config system
	option timezone 'GMT+8'
	option zonename 'GMT+8'
	option ttylogin '1'
	option log_size '256'
	option urandom_seed '0'
	option softVersion 'V1.0.1.299'
	option passwdRecoveryEnable '0'
	option disableTelnet '1'
	option cli_enable '1'
	option disableDebugBootUp '1'
	option netmode 'ap'
	option voslog_support_process 'afu,devProbe,enigma,fota,ledControl,powerdetection,uhttpd'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '1'
	option period '120'
	option server_type '0'
	option tz_mode '1'
	option tz_idx '4'
	option dst_enable '0'
	list server 'time-b.netgear.com'

config webled 'leds'
	option power '1'
	option other '1'

config netgear 'logs'
	option log_site_allowed '1'
	option log_site_block '1'
	option log_login_web '1'
	option log_router_op '1'
	option log_dos '1'
	option log_port '1'
	option log_wire_conn '1'
	option log_wire_sche '1'
	option log_vpn '1'
	option log_service_block '1'
