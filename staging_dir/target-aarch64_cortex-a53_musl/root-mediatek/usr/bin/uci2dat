        merge_profile(vifscfg, t)

        -- fetch all rest keys
        t = vif_cfg_all(vifs)
        merge_profile(vifscfg, t)

        -- dump all we've got
        --for k,v in __spairs(vifscfg) do print(k.."="..v) end

